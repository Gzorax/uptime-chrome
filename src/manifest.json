{
  "manifest_version": 2,
  "name": "Uptime",
  "version": "0.1",
  "description": "Tracks browser uptime",
  "icons": { 
    "16": "icon_16.png"
    "48": "icon_48.png"
    "128": "icon_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["uptime.js"]
  },
  "permissions": ["http://*.google.com/", "https://*.google.com/"]
}